   new users
   <form action="/users/validate" method="POST">
        <label>First name
            <input type="text" name="firstName" value="<%= locals.firstName %>" />
        </label>
        <br/>
        <label>Last name
            <input type="text" name="lastName" value="<%= locals.lastName %>" />
        </label>
        <br/>
        <label>Email
            <input type="text" name="email" value="<%= locals.email %>" />
        </label>
        <br/>
        <label>Username
          <input type="text" name="username" value="<%= locals.username %>" />
        </label>
        <br/>
        <label>Password
            <input type="password" name="password" value="" />
        </label>
        <button type="submit">Submit</button>
    </form>
  </body>
